# -*- YAML -*-
# This is a Spack Environment file.
#
# It describes a set of packages to be installed, along with
# configuration settings.
spack:
  definitions:
  - compiler: [${NATIVE_COMPILER_SPEC}]
  specs:
  - >-
    ${MAIN_COMPILER_BUILD_SPEC} %${NATIVE_COMPILER_SPEC}
  view: false
${EXTRA_ENV_CONFIG}
  include:
  - 'packages.yaml'
